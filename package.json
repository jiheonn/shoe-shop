{
  "name": "backend-interview",
  "version": "1.0.0",
  "repository": "https://github.com/Jiheon-Lee/backend-interview.git",
  "author": "Jiheon-Lee <wlgjsdl0@gmail.com>",
  "private": true,
  "engines": {
    "node": "v14.15.3"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/bin/www.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && NODE_ENV=production babel ./src --out-dir dist --copy-files",
    "start": "pm2 start ecosystem.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "./node_modules/.bin/eslint --fix",
      "./node_modules/.bin/prettier --write"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "pm2": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2"
  }
}
